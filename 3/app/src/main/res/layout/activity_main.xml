<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dolar_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageViewDolar"
        app:layout_constraintStart_toStartOf="@+id/imageViewDolar"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDolar" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/centavos_25"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView25Centavos"
        app:layout_constraintStart_toStartOf="@+id/imageView25Centavos"
        app:layout_constraintTop_toBottomOf="@+id/imageView25Centavos" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/centavos_05"
        app:layout_constraintBottom_toTopOf="@+id/imageView25Centavos"
        app:layout_constraintEnd_toEndOf="@+id/imageView5Centavos"
        app:layout_constraintStart_toStartOf="@+id/imageView5Centavos"
        app:layout_constraintTop_toBottomOf="@+id/imageView5Centavos" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/centavos_10"
        app:layout_constraintBottom_toTopOf="@+id/imageViewDolar"
        app:layout_constraintEnd_toEndOf="@+id/imageView10Centavos"
        app:layout_constraintStart_toStartOf="@+id/imageView10Centavos"
        app:layout_constraintTop_toBottomOf="@+id/imageView10Centavos" />

    <ImageView
        android:id="@+id/imageViewDolar"
        android:layout_width="121dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView25Centavos"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/one_dollar" />

    <ImageView
        android:id="@+id/imageView10Centavos"
        android:layout_width="121dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView5Centavos"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/ten_cents" />

    <ImageView
        android:id="@+id/imageView25Centavos"
        android:layout_width="121dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/imageViewDolar"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/quarter" />

    <ImageView
        android:id="@+id/imageView5Centavos"
        android:layout_width="121dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/imageView10Centavos"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/five_cents" />

    <TextView
        android:id="@+id/Calcular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/top_marign"
        android:text="0"
        android:textSize="@dimen/d_caldular"
        app:layout_constraintEnd_toEndOf="@+id/imageView10Centavos"
        app:layout_constraintStart_toStartOf="@+id/imageView5Centavos"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/top_marign"
        android:text="@string/Titulo"
        android:textSize="@dimen/d_titulo"
        app:layout_constraintBottom_toTopOf="@+id/Calcular"
        app:layout_constraintEnd_toEndOf="@+id/Calcular"
        app:layout_constraintStart_toStartOf="@+id/Calcular"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>